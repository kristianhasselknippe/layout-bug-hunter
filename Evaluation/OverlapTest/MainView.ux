<App>
	<TextInput ux:Class="Text" TextColor="{Property Color}" Color="Black" >
		<float4 ux:Property="Color" />
	</TextInput>

	<Rectangle ux:Class="Wrapper" Color="#FFDEDE" CornerRadius="5" Width="55%" Height="50%"/>

	<Rectangle ux:Class="Rect" CornerRadius="5" />

	<GUITestOracle ux:Name="testOracle"/>

	<Grid RowCount="4" Margin="0,0,0,20">
		<Panel Padding="50,30">
			<Rect Color="#FF5200" Width="70%" Alignment="Left"/>
			<Rect Color="#003355" Width="70%" Alignment="Right"/>
		</Panel>
		<Panel>
			<Rect Color="#ADACA7" Width="40%"/>
			<Rect Color="#003355" Width="70%"/>
		</Panel>
		<Panel Padding="20">
			<Rect Color="#ADACA7" Width="30%"/>
			<Rect Color="#04536C" Height="30%"/>
		</Panel>
		<Panel Padding="40,20" Width="200">
			<Circle Width="70" Height="62" Color="#FF5200" Alignment="TopLeft"/>
			<Circle Width="70" Height="62" Color="#04536C" Alignment="BottomRight"/>
		</Panel>
	</Grid>
</App>
